{
  "entryPoints": ["composables/index.ts"],
  "out": "reference/composables",
  "excludeExternals": true,
  "plugin": ["typedoc-plugin-markdown", "typedoc-vitepress-theme"],
  "githubPages": false,
  "readme": "none",
  "exclude": [
    "**/__tests__/**",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/node_modules/**",
    "**/cypress/**",
    "**/e2e/**",
    "**/mocks/**",
    "**/*.vue"
  ],
  "excludeProtected": true,
  "excludePrivate": true,
  "skipErrorChecking": true,
  "compilerOptions": {
    "skipLibCheck": true
  }
}
