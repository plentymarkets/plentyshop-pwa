    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:cypress": "cypress run --browser chrome --headless",
    "test:cypress-dev": "cypress open --e2e"
  },
  "dependencies": {
