    "cypress-real-events": "^1.10.0",
    "cypress-wait-until": "^2.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-prettier": "5.0.1",
    "happy-dom": "^12.0.0",
    "nuxt": "^3.6.5",
    "nuxt-vitest": "^0.10.1",
    "sass": "^1.64.1",
