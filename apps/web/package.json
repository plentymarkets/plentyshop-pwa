{
  "name": "web",
  "type": "module",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "build": "tsx ./build/index.ts && nuxt build",
    "dev": "tsx ./build/index.ts && nuxt dev",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "docs:dev": "yarn publish-typedoc && vitepress dev docs",
    "docs:build": "yarn publish-typedoc && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "publish-typedoc": "yarn typedoc && mkdir -p docs/reference && mv reference/composables/ docs/reference",
    "lint": "eslint --ext .vue,.js,.ts .",
    "lint:fix": "eslint --ext .vue,.js,.ts . --fix",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:cypress-editor": "cypress run --browser chrome --headless --spec '__tests__/test/editor/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-quarantine": "cypress run --browser chrome --headless --spec '__tests__/test/quarantine/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress": "cypress run --browser chrome --headless --spec '__tests__/test/feature/*.cy.{js,jsx,ts,tsx},__tests__/test/smoke/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-dev": "cypress open --e2e"
  },
  "dependencies": {
    "@plentymarkets/tailwind-colors": "^1.0.0",
    "@vue-storefront/sdk": "^1.5.0",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.7.9",
    "lodash-es": "^4.17.21",
    "nuxt-lazy-hydrate": "^1.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nuxt/image": "^1.8.1",
    "@nuxt/test-utils": "^3.15.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "8.5.6",
    "@nuxtjs/sitemap": "^5.3.5",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@nuxtjs/turnstile": "^0.9.11",
    "@savvywombat/tailwindcss-grid-areas": "^3.1.0",
    "@storefront-ui/typography": "^2.6.1",
    "@storefront-ui/vue": "^2.6.4",
    "@types/culori": "^2.1.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.12",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vitest/coverage-v8": "^0.33.0",
    "@vue-storefront/middleware": "5.3.2",
    "@vue-storefront/nuxt": "^3.1.1",
    "@vue-storefront/unified-data-model": "^0.18.1",
    "@vue/test-utils": "^2.4.6",
    "cypress": "13.17.0",
    "cypress-real-events": "^1.13.0",
    "cypress-wait-until": "^2.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "happy-dom": "^15.11.7",
    "msw": "^2.7.0",
    "nuxt": "^3.15.1",
    "sass": "^1.83.1",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.4.1",
    "typedoc-vitepress-theme": "1.1.2",
    "vitepress": "^1.5.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.2.0",
    "vuepress": "^2.0.0-rc.19"
  }
}
