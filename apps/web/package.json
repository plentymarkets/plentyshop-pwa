{
  "name": "web",
  "type": "module",
  "version": "1.15.0",
  "private": true,
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsx ./build/index.ts && nuxt build",
    "dev": "tsx ./build/index.ts && nuxt dev",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "docs:dev": "npm run publish-typedoc && vitepress dev docs",
    "docs:build": "npm run publish-typedoc && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "publish-typedoc": "npx typedoc --options typedoc.json && mkdir -p docs/reference && rsync -av --delete reference/composables/ docs/reference/composables/",
    "lint": "nuxi prepare && eslint . --max-warnings 0",
    "lint:fix": "nuxi prepare && eslint . --fix --max-warnings 0",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:cypress-checkout": "cypress run --browser chrome --headless --spec '__tests__/test/checkout/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-external": "cypress run --browser chrome --headless --spec '__tests__/test/paypal/*.cy.{js,jsx,ts,tsx},__tests__/test/dhl/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-feature": "cypress run --browser chrome --headless --spec '__tests__/test/feature/*.cy.{js,jsx,ts,tsx},__tests__/test/checkout/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-paypal": "cypress run --browser chrome --headless --spec '__tests__/test/paypal/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-dhl": "cypress run --browser chrome --headless --spec '__tests__/test/dhl/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-editor": "cypress run --browser chrome --headless --spec '__tests__/test/editor/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-quarantine": "cypress run --browser chrome --headless --spec '__tests__/test/quarantine/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-post-quarantine": "cypress run --browser chrome --headless --spec '__tests__/test/post-quarantine/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress": "cypress run --browser chrome --headless --spec '__tests__/test/smoke/*.cy.{js,jsx,ts,tsx}'",
    "test:cypress-dev": "cypress open --e2e"
  },
  "dependencies": {
    "@plentymarkets/shop-module-mollie": "^1.4.0",
    "@plentymarkets/shop-module-gtag": "^1.1.2",
    "@plentymarkets/shop-core": "^1.14.2",
    "@nuxt/fonts": "^0.12.1",
    "@plentymarkets/tailwind-colors": "^1.0.1",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.12.0",
    "nuxt-lazy-hydrate": "^1.0.0",
    "uuid": "^9.0.1",
    "@tanstack/vue-virtual": "^3.13.12",
    "@vee-validate/nuxt": "^4.15.1",
    "@vee-validate/yup": "^4.15.1",
    "blaze-slider": "^1.9.3",
    "country-flag-icons": "^1.5.21",
    "dotenv": "^16.6.1",
    "drift-zoom": "^1.5.1",
    "nuxt-swiper": "^1.2.2",
    "vue-multiselect": "^3.4.0",
    "vue-tel-input": "^9.5.0",
    "vue-tsc": "^3.0.6",
    "vuedraggable": "^4.1.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxtjs/i18n": "^9.5.6",
    "@nuxtjs/sitemap": "^5.2.2",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@savvywombat/tailwindcss-grid-areas": "^3.1.0",
    "@storefront-ui/typography": "^2.6.1",
    "@storefront-ui/vue": "^2.6.3",
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vue-storefront/eslint-config": "^5.1.1",
    "@vue-storefront/middleware": "5.4.1",
    "@vue-storefront/unified-data-model": "^3.0.0",
    "@nuxtjs/turnstile": "^1.1.1",
    "@paypal/paypal-js": "8.4.2",
    "@types/google.maps": "^3.58.1",
    "@types/uuid": "^9.0.8",
    "@types/applepayjs": "^14.0.8",
    "@types/drift-zoom": "^1.5.2",
    "@types/googlepay": "^0.7.8",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vue/test-utils": "^2.4.6",
    "cypress-iframe": "^1.0.1",
    "cypress": "14.5.4",
    "cypress-real-events": "^1.15.0",
    "cypress-wait-until": "^2.0.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^20.0.10",
    "msw": "^2.11.6",
    "nuxt-security": "2.4.0",
    "nuxt-viewport": "^2.3.1",
    "nuxt": "^4.1.3",
    "sass": "^1.93.2",
    "tsx": "^4.20.6",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.1",
    "vuepress": "^2.0.0-rc.24",
    "vuetify-nuxt-module": "^0.18.8",
    "playwright-core": "^1.56.1"
  },
  "overrides": {
    "@vue-storefront/eslint-config": {
      "prettier": "3.6.2"
    },
    "@vitejs/plugin-vue-jsx": "5.0.1"
  }
}