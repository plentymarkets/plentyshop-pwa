    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxtjs/google-fonts": "^3.0.1",
    "@nuxtjs/i18n": "8.0.0-rc.3",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@savvywombat/tailwindcss-grid-areas": "^3.1.0",
    "@storefront-ui/typography": "^2.3.1",
